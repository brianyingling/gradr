.row
  .large-12.columns
    #anonymous
    = link_to('Add Student', new_student_path, :remote => true, :class => 'button tiny radius', :class => 'button_font')
    = link_to('Delete Student', @student, :method=>'delete', :remote => true, :class => 'button tiny radius', :class => 'button_font')
    %h4 Students
    %ul
      - @students.each do |student|
        %li= link_to "#{student.last_name}, #{student.first_name}", student, :remote=>true
#form
#container